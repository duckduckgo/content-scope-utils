{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "RemoteResourceRef",
  "properties": {
    "id": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "name": {
      "type": "string",
      "description": "How this resources is referred to in the UI."
    },
    "kind": {
      "$ref": "resource-kind.json"
    }
  },
  "required": ["id", "url", "name", "kind"],
  "additionalProperties": false
}
